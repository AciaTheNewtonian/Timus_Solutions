cipher = str(input())
lst = [i for i in cipher]
count = 1
while count != 0:
    count = 0
    for x in range(1, len(lst)):
        if lst[x-1] == lst[x]:
            lst[x-1] = ''
            lst[x] = ''
            count += 1
    for k in range(lst.count('')):
        lst.remove('')
for c in lst:
    print(c, end='')
